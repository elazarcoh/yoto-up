# Environment variables for Yoto Web Server

# Server configuration
YOTO_WEB_HOST=127.0.0.1
YOTO_WEB_PORT=8000
YOTO_WEB_DEBUG=false

# Session encryption key (required)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
SESSION_ENCRYPTION_KEY=

# Yoto API (optional - defaults shown)
# YOTO_API_BASE_URL=https://api.yotoplay.com
# YOTO_AUTH_BASE_URL=https://api.yotoplay.com/auth
# YOTO_CLIENT_ID=
# YOTO_CLIENT_SECRET=
